
#oc new-project qs-jakarta
export JAVA_HOME=/usr/lib/jvm/java-8-openjdk-amd64/jre
mvn clean package
docker build -t quicksec-jakarta .
docker tag quicksec-jakarta hyc-wassvt-team-image-registry-docker-local.artifactory.swg-devops.com/quicksec/quicksec-jakarta:java8
docker push  hyc-wassvt-team-image-registry-docker-local.artifactory.swg-devops.com/quicksec/quicksec-jakarta:java8
oc apply -f app-deploy.yaml
sleep 10
oc secrets link quicksec-jakarta  artifactory --for=pull
